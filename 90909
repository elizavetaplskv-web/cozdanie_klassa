class SoyuzDocking:
    def __init__(self):
        self.distance = 500  # –º–µ—Ç—Ä–æ–≤
        self.speed = 50      # –º/—Å
        self.fuel = 100      # –∫–≥
        self.game_over = False
        self.win = False
    def get_status(self):
        return {
            'distance': self.distance,
            'speed': self.speed,
            'fuel': self.fuel,
            'game_over': self.game_over,
            'win': self.win
        }
    def is_valid_move(self, fuel_to_burn):
        if fuel_to_burn < 0:
            print("–ù–µ–ª—å–∑—è —Å–∂–µ—á—å –æ—Ç—Ä–∏—Ü–∞—Ç–µ–ª—å–Ω–æ–µ –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ —Ç–æ–ø–ª–∏–≤–∞!")
            return False
        if fuel_to_burn > self.fuel:
            print("–ù–µ–¥–æ—Å—Ç–∞—Ç–æ—á–Ω–æ —Ç–æ–ø–ª–∏–≤–∞!")
            return False
        if fuel_to_burn > self.speed:
            print("–ù–µ–ª—å–∑—è –∑–∞–º–µ–¥–ª–∏—Ç—å—Å—è –±–æ–ª—å—à–µ, —á–µ–º —Ç–µ–∫—É—â–∞—è —Å–∫–æ—Ä–æ—Å—Ç—å!")
            return False
        return True
    
    def make_move(self, fuel_to_burn):
        """–í—ã–ø–æ–ª–Ω—è–µ—Ç —Ö–æ–¥ –∏–≥—Ä—ã"""
        if self.game_over:
            print("–ò–≥—Ä–∞ —É–∂–µ –æ–∫–æ–Ω—á–µ–Ω–∞!")
            return
        
        if not self.is_valid_move(fuel_to_burn):
            return
        self.fuel -= fuel_to_burn
        self.speed -= fuel_to_burn
        self.distance -= self.speed
        print(f"–°–æ–∂–∂–µ–Ω–æ {fuel_to_burn} –∫–≥ —Ç–æ–ø–ª–∏–≤–∞. –°–∫–æ—Ä–æ—Å—Ç—å: {self.speed} –º/—Å")
        print(f"–û—Å—Ç–∞–ª–æ—Å—å —Ç–æ–ø–ª–∏–≤–∞: {self.fuel} –∫–≥")
        print(f"–†–∞—Å—Å—Ç–æ—è–Ω–∏–µ –¥–æ —Å—Ç–∞–Ω—Ü–∏–∏: {self.distance} –º")
        print("-" * 40)
        self._check_game_conditions()
    def _check_game_conditions(self):
        """–ü—Ä–æ–≤–µ—Ä—è–µ—Ç —É—Å–ª–æ–≤–∏—è –ø–æ–±–µ–¥—ã/–ø—Ä–æ–∏–≥—Ä—ã—à–∞"""
        if self.speed > self.distance:
            self.game_over = True
            print("–°–¢–û–õ–ö–ù–û–í–ï–ù–ò–ï! –°–∫–æ—Ä–æ—Å—Ç—å –ø—Ä–µ–≤—ã—à–∞–µ—Ç –æ—Å—Ç–∞–≤—à–µ–µ—Å—è —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ.")
            print("–ö–æ—Ä–∞–±–ª—å –≤—Ä–µ–∑–∞–ª—Å—è –≤ –∫–æ—Å–º–∏—á–µ—Å–∫—É—é —Å—Ç–∞–Ω—Ü–∏—é!")
            return
        if self.fuel <= 0 and self.distance >= 11:
            self.game_over = True
            print("–ù–ï–¢ –¢–û–ü–õ–ò–í–ê! –ö–æ—Ä–∞–±–ª—å –Ω–µ –º–æ–∂–µ—Ç –∑–∞–≤–µ—Ä—à–∏—Ç—å —Å—Ç—ã–∫–æ–≤–∫—É.")
            print("–ú–∏—Å—Å–∏—è –ø—Ä–æ–≤–∞–ª–µ–Ω–∞!")
            return
        if self.distance < 11:
            if self.speed <= self.distance:
                self.win = True
                self.game_over = True
                print("–£–°–ü–ï–®–ù–ê–Ø –°–¢–´–ö–û–í–ö–ê!")
                print("–ê–≤—Ç–æ–ø–∏–ª–æ—Ç –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω! –ö–æ—Ä–∞–±–ª—å —É—Å–ø–µ—à–Ω–æ —Å–æ—Å—Ç—ã–∫–æ–≤–∞–ª—Å—è —Å —Å—Ç–∞–Ω—Ü–∏–µ–π!")
                if self.fuel > 0:
                    print(f"–û—Å—Ç–∞—Ç–æ–∫ —Ç–æ–ø–ª–∏–≤–∞: {self.fuel} –∫–≥")
            else:
                self.game_over = True
                print("–°–¢–û–õ–ö–ù–û–í–ï–ù–ò–ï –ü–†–ò –°–¢–´–ö–û–í–ö–ï!")
                print("–°–∫–æ—Ä–æ—Å—Ç—å —Å–ª–∏—à–∫–æ–º –≤—ã—Å–æ–∫–∞ –¥–ª—è –∞–∫—Ç–∏–≤–∞—Ü–∏–∏ –∞–≤—Ç–æ–ø–∏–ª–æ—Ç–∞!")
    def display_instructions(self):
        """–û—Ç–æ–±—Ä–∞–∂–∞–µ—Ç –∏–Ω—Å—Ç—Ä—É–∫—Ü–∏–∏ –∫ –∏–≥—Ä–µ"""
        print("=" * 60)
        print("         –°–¢–´–ö–û–í–ö–ê –ö–û–°–ú–ò–ß–ï–°–ö–û–ì–û –ö–û–†–ê–ë–õ–Ø '–°–û–Æ–ó'")
        print("=" * 60)
        print("–¶–ï–õ–¨: –ê–∫–∫—É—Ä–∞—Ç–Ω–æ —Å–æ—Å—Ç—ã–∫–æ–≤–∞—Ç—å—Å—è —Å –∫–æ—Å–º–∏—á–µ—Å–∫–æ–π —Å—Ç–∞–Ω—Ü–∏–µ–π")
        print("\n–ü–†–ê–í–ò–õ–ê:")
        print("- –ù–∞—á–∞–ª—å–Ω–æ–µ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ: 500 –º, —Å–∫–æ—Ä–æ—Å—Ç—å: 50 –º/—Å")
        print("- –¢–æ–ø–ª–∏–≤–æ: 100 –∫–≥ (1 –∫–≥ —Ç–æ–ø–ª–∏–≤–∞ = –∑–∞–º–µ–¥–ª–µ–Ω–∏–µ –Ω–∞ 1 –º/—Å)")
        print("- –î–ª—è —É—Å–ø–µ—Ö–∞ –Ω—É–∂–Ω–æ:")
        print("  1. –ü–æ–¥–ª–µ—Ç–µ—Ç—å –±–ª–∏–∂–µ 11 –º –∫ —Å—Ç–∞–Ω—Ü–∏–∏")
        print("  2. –°–∫–æ—Ä–æ—Å—Ç—å –Ω–µ –¥–æ–ª–∂–Ω–∞ –ø—Ä–µ–≤—ã—à–∞—Ç—å –æ—Å—Ç–∞–≤—à–µ–µ—Å—è —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ")
        print("  3. –ù–µ –∏–∑—Ä–∞—Å—Ö–æ–¥–æ–≤–∞—Ç—å –≤—Å—ë —Ç–æ–ø–ª–∏–≤–æ –¥–æ —Å—Ç—ã–∫–æ–≤–∫–∏")
        print("\n–£–î–ê–ß–ò!")
        print("=" * 60)
        print()
def main():
    game = SoyuzDocking()
    game.display_instructions()
    while not game.game_over:
        status = game.get_status()
        print(f"–†–∞—Å—Å—Ç–æ—è–Ω–∏–µ: {status['distance']} –º | "
              f"–°–∫–æ—Ä–æ—Å—Ç—å: {status['speed']} –º/—Å | "
              f"–¢–æ–ø–ª–∏–≤–æ: {status['fuel']} –∫–≥")
        try:
            fuel_input = input("–°–∫–æ–ª—å–∫–æ —Ç–æ–ø–ª–∏–≤–∞ —Å–∂–µ—á—å (–∫–≥)? ")
            fuel_to_burn = int(fuel_input)
            game.make_move(fuel_to_burn)
        except ValueError:
            print("–ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ —Ü–µ–ª–æ–µ —á–∏—Å–ª–æ!")
        except KeyboardInterrupt:
            print("\n–ò–≥—Ä–∞ –ø—Ä–µ—Ä–≤–∞–Ω–∞ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–º")
            break
    if game.win:
        print("\nüéâ –ü–û–ó–î–†–ê–í–õ–Ø–ï–ú! –ú–∏—Å—Å–∏—è –≤—ã–ø–æ–ª–Ω–µ–Ω–∞ —É—Å–ø–µ—à–Ω–æ! üéâ")
    elif game.game_over and not game.win:
        print("\nüí• –ú–∏—Å—Å–∏—è –ø—Ä–æ–≤–∞–ª–µ–Ω–∞. –ü–æ–ø—Ä–æ–±—É–π—Ç–µ –µ—â–µ —Ä–∞–∑! üí•")
if __name__ == "__main__":
    main()
    # –°–æ–∑–¥–∞—Ç—å –ø–æ—Å–ª–µ–¥–æ–≤–∞—Ç–µ–ª—å–Ω–æ—Å—Ç—å —Å—Ç—ã–∫–æ–≤–∫–∏
docking_sequence = SoyuzDocking()
print(f"–†–∞—Å—Å—Ç–æ—è–Ω–∏–µ: {docking_sequence.distance}")
print(f"–°–∫–æ—Ä–æ—Å—Ç—å: {docking_sequence.speed}") 
print(f"–¢–æ–ø–ª–∏–≤–æ: {docking_sequence.fuel}")
docking_sequence.make_move(10)  # —Å–∂–∏–≥–∞–µ–º 10 –∫–≥
# –°–º–æ—Ç—Ä–∏–º —á—Ç–æ –∏–∑–º–µ–Ω–∏–ª–æ—Å—å
print(f"–¢–µ–ø–µ—Ä—å —Å–∫–æ—Ä–æ—Å—Ç—å: {docking_sequence.speed}")  # —Å—Ç–∞–ª–∞ 40
print(f"–¢–æ–ø–ª–∏–≤–∞ –æ—Å—Ç–∞–ª–æ—Å—å: {docking_sequence.fuel}")  # 90
print(f"–†–∞—Å—Å—Ç–æ—è–Ω–∏–µ: {docking_sequence.distance}")    # 460
# –ï—â–µ —Ä–∞–∑ –∂–∂–µ–º —Ç–æ–ø–ª–∏–≤–æ
docking_sequence.make_move(15)
# –ü—Ä–æ–≤–µ—Ä—è–µ–º –Ω–µ –∑–∞–∫–æ–Ω—á–∏–ª–∞—Å—å –ª–∏ –∏–≥—Ä–∞
if docking_sequence.game_over:
    print("–ò–≥—Ä–∞ –∫–æ–Ω—á–∏–ª–∞—Å—å!")
    if docking_sequence.win:
        print("–¢—ã –≤—ã–∏–≥—Ä–∞–ª!")
    else:
        print("–¢—ã –ø—Ä–æ–∏–≥—Ä–∞–ª!")
else:
    print("–ò–≥—Ä–∞ –µ—â–µ –∏–¥–µ—Ç")
# –ú–æ–∂–Ω–æ –¥–µ–ª–∞—Ç—å —Ç–∞–∫ –≤ —Ü–∏–∫–ª–µ –ø–æ–∫–∞ –∏–≥—Ä–∞ –Ω–µ –∫–æ–Ω—á–∏—Ç—Å—è
while not docking_sequence.game_over:
    # —Ç—É—Ç –≤–≤–æ–¥–∏–º —Å–∫–æ–ª—å–∫–æ —Ç–æ–ø–ª–∏–≤–∞ –∂–µ—á—å
    docking_sequence.make_move(5)  
    class SoyuzDocking:
    def __init__(self):
        self.distance = 500  # –º–µ—Ç—Ä–æ–≤ –¥–æ —Å—Ç–∞–Ω—Ü–∏–∏
        self.speed = 50      # –º/—Å
        self.fuel = 100      # –∫–≥
    
    def perform_burn(self, burn_amount):
        """–°–∂–µ—á—å —Ç–æ–ø–ª–∏–≤–æ –¥–ª—è –∑–∞–º–µ–¥–ª–µ–Ω–∏—è"""
        self.speed = max(self.speed - burn_amount, 0)
        self.fuel = max(self.fuel - burn_amount, 0)
    
    def update_distance(self):
        """–õ–µ—Ç–∏–º –≤–ø–µ—Ä–µ–¥ –Ω–∞ —Ç–µ–∫—É—â–µ–π —Å–∫–æ—Ä–æ—Å—Ç–∏"""
        self.distance = max(self.distance - self.speed, 0)
    
    def has_docked(self):
        """–ü—Ä–æ–≤–µ—Ä–∏—Ç—å –¥–æ–ª–µ—Ç–µ–ª–∏ –ª–∏ –¥–æ —Å—Ç–∞–Ω—Ü–∏–∏"""
        return self.distance <= 0

# –°–æ–∑–¥–∞–µ–º –∏–≥—Ä—É
docking_sequence = SoyuzDocking()

# –ü–æ–∫–∞–∑–∞—Ç—å –∏–Ω—Å—Ç—Ä—É–∫—Ü–∏–∏ –∫ –∏–≥—Ä–µ
print("–î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å –≤ —Å–∏–º—É–ª—è—Ü–∏—é —Å—Ç—ã–∫–æ–≤–∫–∏ –°–æ—é–∑ –¢-6!")
print("–í–∞—à–∞ –º–∏—Å—Å–∏—è ‚Äì —Å—Ç—ã–∫–æ–≤–∫–∞ —Å–æ —Å—Ç–∞–Ω—Ü–∏–µ–π –°–∞–ª—é—Ç-7.")
print("–í—ã –º–æ–∂–µ—Ç–µ —É–ø—Ä–∞–≤–ª—è—Ç—å —Å–∫–æ—Ä–æ—Å—Ç—å—é –∫–æ—Å–º–∏—á–µ—Å–∫–æ–≥–æ –∫–æ—Ä–∞–±–ª—è —Å–∂–∏–≥–∞—è —Ç–æ–ø–ª–∏–≤–æ.")
print("–ö–∞–∂–¥–∞—è –µ–¥–∏–Ω–∏—Ü–∞ —Å–æ–∂–∂–µ–Ω–Ω–æ–≥–æ —Ç–æ–ø–ª–∏–≤–∞ –∑–∞–º–µ–¥–ª—è–µ—Ç –∫–æ—Å–º–∏—á–µ—Å–∫–∏–π –∫–æ—Ä–∞–±–ª—å –Ω–∞ 1 –º/—Å.")
print("–£–¥–∞—á–∏ —ç–∫–∏–ø–∞–∂—É!\n")

# –ì–ª–∞–≤–Ω—ã–π –∏–≥—Ä–æ–≤–æ–π —Ü–∏–∫–ª
while not docking_sequence.has_docked():
    # –ü–æ–∫–∞–∑—ã–≤–∞–µ–º —Ç–µ–∫—É—â–µ–µ —Å–æ—Å—Ç–æ—è–Ω–∏–µ
    print(f"–†–∞—Å—Å—Ç–æ—è–Ω–∏–µ –¥–æ –°–∞–ª—é—Ç-7: {docking_sequence.distance} –º–µ—Ç—Ä–æ–≤")
    print(f"–°–∫–æ—Ä–æ—Å—Ç—å: {docking_sequence.speed} –º/—Å")
    print(f"–¢–æ–ø–ª–∏–≤–æ: {docking_sequence.fuel} –∫–≥")
    
    # –°–æ–æ–±—â–µ–Ω–∏–µ –æ –ø—Ä–æ–≤–∞–ª–µ –º–∏—Å—Å–∏–∏ –≤ —Å–ª—É—á–∞–µ –µ—Å–ª–∏ –∑–∞–∫–æ–Ω—á–∏–ª–æ—Å—å —Ç–æ–ø–ª–∏–≤–æ
    if docking_sequence.fuel <= 0:
        print("–ö–æ–Ω—á–∏–ª–æ—Å—å —Ç–æ–ø–ª–∏–≤–æ!")
        break
    
    # –ó–∞–ø—Ä–æ—Å –Ω–∞ –∞–∫—Ç–∏–≤–∞—Ü–∏—é –∞–≤—Ç–æ–ø–∏–ª–æ—Ç–∞ –µ—Å–ª–∏ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ –¥–æ —Å—Ç–∞–Ω—Ü–∏–∏ –º–µ–Ω–µ–µ 11 –º
    if docking_sequence.distance < 11:
        autopilot = input("–î–æ —Å—Ç–∞–Ω—Ü–∏–∏ –°–∞–ª—é—Ç-7 –æ—Å—Ç–∞–ª–æ—Å—å –º–µ–Ω–µ–µ 11 –º–µ—Ç—Ä–æ–≤. –ê–∫—Ç–∏–≤–∏—Ä–æ–≤–∞—Ç—å —Ä–µ–∂–∏–º –∞–≤—Ç–æ–ø–∏–ª–æ—Ç–∞ –¥–ª—è –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–π —Å—Ç—ã–∫–æ–≤–∫–∏? (–¥–∞/–Ω–µ—Ç): ")
        if autopilot.lower() == '–¥–∞':
            print("–ê–≤—Ç–æ–ø–∏–ª–æ—Ç –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω.")
            break
        else:
            print("–ü—Ä–æ–¥–æ–ª–∂–∞–µ–º —Ä—É—á–Ω–æ–µ —É–ø—Ä–∞–≤–ª–µ–Ω–∏–µ.")
    
    # –ò–≥—Ä–æ–∫ –≤–≤–æ–¥–∏—Ç —Å–∫–æ–ª—å–∫–æ —Ç–æ–ø–ª–∏–≤–∞ –∂–µ—á—å
    try:
        burn_amount = int(input("–°–∫–æ–ª—å–∫–æ —Ç–æ–ø–ª–∏–≤–∞ —Å–∂–µ—á—å –¥–ª—è –∑–∞–º–µ–¥–ª–µ–Ω–∏—è? "))
        
        # –ü—Ä–æ–≤–µ—Ä—è–µ–º —á—Ç–æ —Ö–æ–¥ –≤–∞–ª–∏–¥–Ω—ã–π
        if burn_amount < 0:
            print("–ù–µ–ª—å–∑—è —Å–∂–µ—á—å –æ—Ç—Ä–∏—Ü–∞—Ç–µ–ª—å–Ω–æ–µ —Ç–æ–ø–ª–∏–≤–æ!")
            continue
        if burn_amount > docking_sequence.fuel:
            print("–ù–µ–¥–æ—Å—Ç–∞—Ç–æ—á–Ω–æ —Ç–æ–ø–ª–∏–≤–∞!")
            continue
        if burn_amount > docking_sequence.speed:
            print("–ù–µ–ª—å–∑—è –∑–∞–º–µ–¥–ª–∏—Ç—å—Å—è –±–æ–ª—å—à–µ —á–µ–º —Ç–µ–∫—É—â–∞—è —Å–∫–æ—Ä–æ—Å—Ç—å!")
            continue
        
        # –í—ã–ø–æ–ª–Ω—è–µ–º —Ö–æ–¥
        docking_sequence.perform_burn(burn_amount)  # –∂–∂–µ–º —Ç–æ–ø–ª–∏–≤–æ
        docking_sequence.update_distance()          # –ª–µ—Ç–∏–º –≤–ø–µ—Ä–µ–¥
        
        print()  # –ø—É—Å—Ç–∞—è —Å—Ç—Ä–æ–∫–∞ –¥–ª—è –∫—Ä–∞—Å–æ—Ç—ã
        
    except ValueError:
        print("–ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –≤–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ!")
    except KeyboardInterrupt:
        print("\n–ò–≥—Ä–∞ –ø—Ä–µ—Ä–≤–∞–Ω–∞")
        break

# –ó–∞–≤–µ—Ä—à–µ–Ω–∏–µ –ø—Ä–æ—Ü–µ—Å—Å–∞ —Å—Ç—ã–∫–æ–≤–∫–∏ ‚Äì –ø—Ä–æ–≤–µ—Ä–∏—Ç—å —É—Å–ª–æ–≤–∏—è –∏ –≤—ã–≤–µ—Å—Ç–∏ —Ä–µ–∑—É–ª—å—Ç–∞—Ç
if docking_sequence.distance <= 11 and docking_sequence.speed <= docking_sequence.distance:
    print("–°—Ç—ã–∫–æ–≤–∫–∞ –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∞. –ü–æ–∑–¥—Ä–∞–≤–ª—è–µ–º —ç–∫–∏–ø–∞–∂!")
else:
    print("–ú–∏—Å—Å–∏—è –ø—Ä–æ–≤–∞–ª–µ–Ω–∞. –°–æ—é–∑ –¢-6 –Ω–µ —Å–º–æ–≥ —Å–æ—Å—Ç—ã–∫–æ–≤–∞—Ç—å—Å—è —Å –°–∞–ª—é—Ç-7.")
